{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "standard"
  ],
  "rules": {
    "space-before-function-paren": "off",
    "@typescript-eslint/no-unused-vars": 2
  },
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
